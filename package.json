{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "proxy-config": "tsc --types node --module commonjs proxy.config.ts",
    "create-environment": "ts-node ./create-environment.ts",
    "start": "npm run create-environment && npm run proxy-config && ng serve --proxy-config proxy.config.js",
    "build": "npm run create-environment && npm run proxy-config && ng build",
    "build-prod": "npm run create-environment && npm run proxy-config && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-development": "ng build --configuration development",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && ng serve -o"
  },
